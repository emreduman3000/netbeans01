<FlowPane prefHeight="350.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="w6fx.FXMLController">
<children>
    <Label text="Enter first number:" />
    <TextField fx:id="txtfirst" />
    <Label text="Enter second number:" />
    <TextField fx:id="txtsecond" />
    <Label text="Result :" />
    <TextField fx:id="txtres" />
